 "css": "\n\t.print-format table, .print-format tr, \n\t.print-format td, .print-format div, .print-format p {\n\t\tvertical-align: middle;\n\t}\n\t@media screen {\n\t\t.print-format {\n\t\t\t/*width: 5.8in;*/\n\t\t\tpadding: 0.40in;\n\t\t\t/*height: 8.3in;*/\n\t\t}\n\t\tbody{\n\t        font-size:5px;\n\t    }\n\t}\n\t@media print {\n\t\t.print-format {\n\t\t\t/*width: 5.8in;*/\n\t\t\tpadding: 0.40in;\n\t\t\t/*height: 8.3in;*/\n\t\t}\n\t\tbody{\n\t        font-size:5px;\n\t    }\n\t    .table-header {\n    \t    background-color: #080808 !important;\n    \t    color: #000000 !important;\n\t    }\n\t    .th-text {\n\t        color: black !important;\n\t    }\n\t}\n\t.table-header {\n\t    background: grey;\n\t    color: #000000;\n\t}\n\t.th-text {\n\t    /*color: white !important;*/\n\t}\n\t.text-lg{\n\t    font-size:25px;\n\t    font-weight: bold;\n\t}\n\t.text-md{\n\t    margin-left:0px;\n\t    font-size:16px;\n\t}\n\t.text-sm{\n        line-height: 12px;\n        font-size:10px;\n\t}\n\t.text-sm2{\n        line-height: 12px !important;\n        font-size:10px !important;\n\t}\n\t\n\t.print-format td, .print-format th {\n        vertical-align: top !important;\n        padding: 4px !important;\n        font-size:10px;\n    }\n    \n    .vertical-center {\n        margin: 0;\n        position: absolute;\n        top: 50%;\n        -ms-transform: translateY(-50%);\n        transform: translateY(-50%);\n    }\n    .ml-12{\n        margin-left:-12px;\n    }\n    .footer-banner{\n    \tbackground: #d2d3d9;\n\t    color: #000000;\n    }",
